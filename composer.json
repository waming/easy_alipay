{
    "name": "honghm/easy_alipay",
    "description": "支付宝SDK",
    "type": "library",
    "keywords": [
        "easyalipay",
        "alipay",
        "openalipay",
        "alipay-sdk",
        "sdk"
    ],
    "require": {
        "php": ">=8.0.2",
        "ext-openssl": "*",
        "ext-mbstring": "*",
        "ext-ctype": "*",
        "ext-bcmath": "*",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0",
        "nyholm/psr7": "1.8.x-dev",
        "nyholm/psr7-server": "^1.1",
        "symfony/http-client": "^6.0",
        "symfony/http-foundation": "^6.0",
        "symfony/psr-http-message-bridge": "^2.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "jetbrains/phpstorm-attributes": "^1.0",
        "mockery/mockery": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0.0",
        "symfony/var-dumper": "^5.1",
        "symfony/event-dispatcher": "^5.2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Honghm\\EasyAlipay\\": "src/"
        },
        "files": [
          "src/Kernel/Support/Functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Honghm\\EasyAlipay\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 300M -l 5 -c phpstan.neon ./src"
    },
    "authors": [
        {
            "name": "honghm",
            "email": "honghua1207@sina.com"
        }
    ],
    "minimum-stability": "dev"
}
